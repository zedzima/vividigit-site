        "parameter": [
          {
            "type": "TEMPLATE",
            "key": "html",
            "value": "<script>\n(function(){\n    window.dataLayer=window.dataLayer||[];\n\n    /* view_cart — detect cart page */\n    function checkCartPage(){\n        if(document.getElementById('cartPage')){\n            dataLayer.push({event:'view_cart'});\n        }\n    }\n    if(document.readyState==='loading'){\n        document.addEventListener('DOMContentLoaded',checkCartPage);\n    }else{\n        checkCartPage();\n    }\n\n    /* Click delegation */\n    document.addEventListener('click',function(e){\n        /* Add to cart — pricing package button */\n        if(e.target.closest('.pricing-package .btn')){\n            dataLayer.push({event:'add_to_cart'});\n            return;\n        }\n        /* Add to cart — pricing tier */\n        if(e.target.closest('.pricing-tier')){\n            dataLayer.push({event:'add_to_cart'});\n            return;\n        }\n        /* Remove from cart — sidebar + cart page */\n        if(e.target.closest('.cart-item-remove')||e.target.closest('.cart-item-remove-btn')){\n            dataLayer.push({event:'remove_from_cart'});\n            return;\n        }\n        /* Begin checkout */\n        if(e.target.closest('#cartSendRequest')){\n            dataLayer.push({event:'begin_checkout'});\n            return;\n        }\n    });\n\n    /* Task picker checkbox — add or remove based on state */\n    document.addEventListener('change',function(e){\n        if(e.target.classList.contains('task-select-cb')){\n            dataLayer.push({event:e.target.checked?'add_to_cart':'remove_from_cart'});\n        }\n    });\n})();\n</script>"
