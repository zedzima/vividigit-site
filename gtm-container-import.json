        "parameter": [
          {
            "type": "TEMPLATE", "key": "html",
            "value": "<script>\n(function(){\n    window.dataLayer=window.dataLayer||[];\n\n    /* === Helper: detect page section from element context === */\n    function getSection(el){\n        if(el.closest('.sidebar-action')) return 'sidebar';\n        if(el.closest('footer')) return 'footer';\n        var block=el.closest('[class*=\"-block\"]');\n        if(block){\n            var m=block.className.match(/(\\S+)-block/);\n            if(m) return m[1];\n        }\n        return 'page';\n    }\n\n    /* === view_cart — detect cart page on DOM ready === */\n    function checkCartPage(){\n        if(document.getElementById('cartPage')){\n            dataLayer.push({event:'view_cart'});\n        }\n    }\n    if(document.readyState==='loading'){\n        document.addEventListener('DOMContentLoaded',checkCartPage);\n    } else { checkCartPage(); }\n\n    /* === Scroll milestone — 70% page depth === */\n    var scrollFired=false;\n    window.addEventListener('scroll',function(){\n        if(scrollFired) return;\n        var pct=(window.scrollY+window.innerHeight)/document.documentElement.scrollHeight*100;\n        if(pct>=70){\n            scrollFired=true;\n            dataLayer.push({event:'scroll_milestone'});\n        }\n    },{passive:true});\n\n    /* === Click delegation === */\n    document.addEventListener('click',function(e){\n\n        /* -- Cart: add_to_cart (pricing package button) -- */\n        if(e.target.closest('.pricing-package .btn')){\n            dataLayer.push({event:'add_to_cart'}); return;\n        }\n        /* -- Cart: add_to_cart (pricing tier) -- */\n        if(e.target.closest('.pricing-tier')){\n            dataLayer.push({event:'add_to_cart'}); return;\n        }\n        /* -- Cart: remove_from_cart (sidebar + cart page) -- */\n        if(e.target.closest('.cart-item-remove')||e.target.closest('.cart-item-remove-btn')){\n            dataLayer.push({event:'remove_from_cart'}); return;\n        }\n        /* -- Cart: begin_checkout -- */\n        if(e.target.closest('#cartSendRequest')){\n            dataLayer.push({event:'begin_checkout'}); return;\n        }\n\n        /* -- Content: process step -- */\n        var proc=e.target.closest('.process-btn');\n        if(proc){\n            dataLayer.push({event:'content_engage',\n                event_label:proc.textContent.trim().substring(0,80),\n                event_section:'process'});\n            return;\n        }\n\n        /* -- Content: FAQ expand (only opens, not closes) -- */\n        var faq=e.target.closest('.faq-question');\n        if(faq){\n            var det=faq.closest('details');\n            if(det&&!det.open){\n                dataLayer.push({event:'content_engage',\n                    event_label:faq.textContent.trim().substring(0,100),\n                    event_section:'faq'});\n            }\n            return;\n        }\n\n        /* -- CTA: link buttons (a.btn) — catch-all for remaining CTAs -- */\n        var cta=e.target.closest('a.btn');\n        if(cta){\n            dataLayer.push({event:'cta_click',\n                event_label:cta.textContent.trim().substring(0,80),\n                event_section:getSection(cta)});\n            return;\n        }\n    });\n\n    /* === Change delegation === */\n    document.addEventListener('change',function(e){\n        /* Task picker checkbox */\n        if(e.target.classList.contains('task-select-cb')){\n            dataLayer.push({event:e.target.checked?'add_to_cart':'remove_from_cart'});\n        }\n    });\n})();\n</script>"
